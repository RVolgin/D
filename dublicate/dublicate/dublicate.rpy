    #########
    #Эффекты#
    #########
    
    transform walking:
        pos (0,0)
        linear 0.3 pos (-4,-2)
        linear 0.3 pos (0,0)
        pos (0,0)
        linear 0.3 pos (4,-2)
        linear 0.3 pos (0,0)
        repeat
        
    transform running:
        zoom 1.05anchor (48,27)
        ease 0.20pos (0, 0)
        ease 0.20pos (25,25)
        ease 0.20pos (0, 0)
        ease 0.20pos (-25,25)
        repeat
        
    transform shiver:
        pos (-2,0)
        linear 0.1 pos (0,0)
        linear 0.1 pos (2,0)
        linear 0.1 pos (0,0)
        linear 0.1 pos (-2,0)
        repeat

